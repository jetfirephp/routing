{
    "name": "jetfirephp/routing",
    "type": "library",
    "description": "JetFire - Routing",
    "keywords": ["Rounting", "Dispatcher", "Micro MVC"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sumugan Sinnarasa",
            "email": "mugan27@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "symfony/yaml": "v2.8.2"
    },
    "suggest": {
        "jetfire/autoload": "For loading your class"
    },
    "autoload": {
        "psr-4":{
            "JetFire\\Routing\\": "src/Routing/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JetFire\\Routing\\Test\\": "tests/src/Routing/",
            "JetFire\\Routing\\App\\": "tests/app/"
        }
    },
    "minimum-stability": "dev"
}
